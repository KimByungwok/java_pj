package com.java.ex;

// 기초 자료형 (= 기본 데이터 타입)               표현 범위
// 1 논리형 boolean = 1byte (= 1 bit)   0은 플러스 1은 마이너스
// 2 문자형 char = 2byte                맨 앞 숫자는 부호 비트
// 3 정수형 byte = 1byte					
// 4 정수형 short = 2byte				   
// 5 정수형 int = 4byte					
// 6 정수형 long = 8byte
// 7 실수형 float = 4byte
// 8 실수형 double = 8byte

// 기본 데이터 타입 계산하는법

public class Java3_Var {

	public static void main(String[] args) {
		System.out.println("변수는 값을 받는 그릇이에오");
		int     a    =   10;
	//데이터 타입_변수명     데이터
	// 그릇 성질_그릇 이름  그릇 내용물
		int b = 20;
		int c = 30;
		
		char d = 'A';  // char 차는 캐릭터(특징,문자)
		char e = 'B';
		char f = 'C';
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		System.out.println(f);
	}
}






















