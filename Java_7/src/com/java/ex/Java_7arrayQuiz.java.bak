package com.java.ex;

public class Java_7arrayQuiz {

	public static void main(String[] args) {
		int[] iArr = {10,20,30,40,50};
		
		for(int i =0;i<5;i++) {
			System.out.print(iArr[i]+"\t");
		}
		System.out.println("");
		
		int[] jArr =new int[3];
		jArr[0]=0;
		jArr[1]=1;
		jArr[2]=2;
		
		for(int j =0;j<3;j++) {
			System.out.println(jArr[j]+"\t");
		}
	}
}
